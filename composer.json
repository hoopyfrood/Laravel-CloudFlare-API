{
    "name": "hoopyfrood/cloudflare-api",
    "description": "CloudFlare API Is A CloudFlare API Client For Laravel 5",
    "keywords": ["laravel", "framework", "cloudflare api", "cloudflare", "api", "CloudFlare API", "CloudFlare-API", "Laravel CloudFlare API", "Laravel-CloudFlare-API", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/contracts": "5.1.*",
        "illuminate/support": "5.1.*",
        "graham-campbell/manager": "~2.0",
        "guzzlehttp/guzzle": "~5.0",
        "guzzlehttp/guzzle-services": "0.5.*",
         "guzzlehttp/retry-subscriber": "~2.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\CloudFlareAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\CloudFlareAPI\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.7-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
